{
    "action_manifest_version": 2,
    "alias_info": {},
    "bindings": {
        "/actions/haptics": {
            "haptics": [
                {
                    "output": "/actions/poses/in/hapticvibration",
                    "path": "/user/hand/left/output/haptic"
                },
                {
                    "output": "/actions/poses/in/hapticvibration",
                    "path": "/user/hand/right/output/haptic"
                }
            ]
        },
        "/actions/poses": {
            "poses": [
                {
                    "output": "/actions/poses/in/handposeleft",
                    "path": "/user/hand/left/pose/handgrip"
                },
                {
                    "output": "/actions/poses/in/handposeright",
                    "path": "/user/hand/right/pose/handgrip"
                },
                {
                    "output": "/actions/poses/in/handpointerleft",
                    "path": "/user/hand/left/pose/tip"
                },
                {
                    "output": "/actions/poses/in/handpointerright",
                    "path": "/user/hand/right/pose/tip"
                }
            ],
            "skeleton": [
                {
                    "output": "/actions/poses/in/handskeletonleft",
                    "path": "/user/hand/left/input/skeleton/left"
                },
                {
                    "output": "/actions/poses/in/handskeletonright",
                    "path": "/user/hand/right/input/skeleton/right"
                }
            ],
            "sources": [
                {
                    "inputs": {
                        "pull": {
                            "output": "/actions/poses/in/triggerpull"
                        }
                    },
                    "mode": "trigger",
                    "path": "/user/hand/left/input/trigger"
                },
                {
                    "inputs": {
                        "pull": {
                            "output": "/actions/poses/in/triggerpull"
                        }
                    },
                    "mode": "trigger",
                    "path": "/user/hand/right/input/trigger"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/poses/in/handcurl"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/right/input/grip"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/poses/in/handcurl"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/grip"
                }
            ]
        },
        "/actions/menu": {
            "sources": [
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/menu/in/togglevrmenu"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/application_menu"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/menu/in/clickvrmenu"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/trigger"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/menu/in/clickvrmenu"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/right/input/trigger"
                }
            ]
        },
        "/actions/interact": {
            "sources": [
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/interact/in/legacyuse"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/trigger"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/interact/in/legacyuse"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/right/input/trigger"
                },
                {
                    "inputs": {
                        "grab": {
                            "output": "/actions/interact/in/grab"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/grip"
                },
                {
                    "inputs": {
                        "grab": {
                            "output": "/actions/interact/in/grab"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/right/input/grip"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/interact/in/teleport"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/trackpad"
                }
            ]
        },
        "/actions/move": {
            "sources": [
                {
                    "inputs": {
                        "position": {
                            "output": "/actions/move/in/move"
                        }
                    },
                    "mode": "joystick",
                    "parameters": {
                        "exponent": "2"
                    },
                    "path": "/user/hand/left/input/joystick"
                },
                {
                    "inputs": {
                        "east": {
                            "output": "/actions/move/in/turnright"
                        },
                        "west": {
                            "output": "/actions/move/in/turnleft"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "deadzone_pct": "75",
                        "overlap_pct": "0",
                        "sticky": "true",
                        "sub_mode": "touch"
                    },
                    "path": "/user/hand/right/input/joystick"
                },
                {
                    "inputs": {
                        "position": {
                            "output": "/actions/move/in/continuousturn"
                        }
                    },
                    "mode": "joystick",
                    "parameters": {
                        "deadzone_pct": "25",
                        "exponent": "2",
                        "sticky_click": "false"
                    },
                    "path": "/user/hand/right/input/joystick"
                },
                {
                    "inputs": {
                        "north": {
                            "output": "/actions/move/in/jump"
                        },
                        "south": {
                            "output": "/actions/move/in/crouch"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "deadzone_pct": "80",
                        "overlap_pct": "0",
                        "sticky": "true",
                        "sub_mode": "touch"
                    },
                    "path": "/user/hand/right/input/joystick"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/move/in/walk"
                        }
                    },
                    "mode": "joystick",
                    "parameters": {
                        "sticky_click": "true"
                    },
                    "path": "/user/hand/left/input/joystick"
                }
            ]
        },
        "/actions/weapon": {
            "sources": [
                {
                    "inputs": {
                        "north": {
                            "output": "/actions/weapon/in/attack2"
                        },
                        "south": {
                            "output": "/actions/weapon/in/reload"
                        }
                    },
                    "mode": "dpad",
                    "parameters": {
                        "overlap_pct": "0",
                        "sticky": "true",
                        "sub_mode": "click"
                    },
                    "path": "/user/hand/right/input/trackpad"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/weapon/in/attack"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/trigger"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/weapon/in/attack"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/right/input/trigger"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/weapon/in/select"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/right/input/application_menu"
                },
                {
                    "inputs": {
                        "click": {
                            "output": "/actions/weapon/in/next"
                        }
                    },
                    "mode": "button",
                    "path": "/user/hand/left/input/x"
                }
            ]
        }
    },
    "category": "steamvr_input",
    "controller_type": "holographic_controller",
    "description": "Default two handed bindings with movement on the off hand",
    "name": "Dual Controllers (Movement on Off Hand)",
    "options": {},
    "simulated_actions": []
}